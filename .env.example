# Environment variables for Docker deployment
# Copy this to .env and fill in your actual values

# Groq AI API Key (Required)
GROQ_API_KEY=your_groq_api_key_here

# ElevenLabs API Key (Optional - for voice)
ELEVEN_API_KEY=your_elevenlabs_key_here

# SMTP Settings for OTP emails (Required)
SMTP_EMAIL=your_gmail@gmail.com
SMTP_PASSWORD=your_app_password_here

# Firebase Project ID (Required)
FIREBASE_PROJECT_ID=predicare-cdc51

# Frontend API URL (Update for production)
VITE_API_URL=http://localhost:8000
